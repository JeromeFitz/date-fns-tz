{
  "name": "date-fns-tz",
  "version": "3.0.0",
  "sideEffects": false,
  "description": "Time zone support for date-fns v2 with the Intl API",
  "author": "Marnus Weststrate <marnusw@gmail.com>",
  "license": "MIT",
  "keywords": [
    "date-fns",
    "timezone",
    "time zone",
    "date",
    "time",
    "parse",
    "format",
    "immutable"
  ],
  "repository": "https://github.com/marnusw/date-fns-tz",
  "engine": {
    "node": ">= 20"
  },
  "main": "./index.js",
  "module": "./index.mjs",
  "scripts": {
    "test": "vitest run",
    "lint": "eslint .",
    "types": "tsc --noEmit",
    "zzz:build": "./scripts/build/build.sh",
    "zzz:package": "./scripts/build/package.sh",
    "zzz:clean": "rm -rf tmp/package && rm tmp/docs.json",
    "zzz:test": "cross-env NODE_OPTIONS=\"--openssl-legacy-provider\" karma start config/karma.js",
    "zzz:test:watch": "cross-env TEST_WATCH=true NODE_OPTIONS=\"--openssl-legacy-provider\" karma start config/karma.js",
    "zzz:prettier": "prettier . --write",
    "zzz:lint": "eslint .",
    "zzz:lint-types": "eslint --config=.ts-eslintrc.js typings.d.ts"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ],
    "*.{js,json,md,ts}": [
      "prettier --write"
    ]
  },
  "peerDependencies": {
    "date-fns": ">=3.0.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.22.10",
    "@babel/core": "^7.22.10",
    "@babel/node": "^7.22.19",
    "@babel/plugin-transform-arrow-functions": "^7.14.5",
    "@babel/plugin-transform-block-scoping": "^7.15.3",
    "@babel/plugin-transform-destructuring": "^7.14.7",
    "@babel/plugin-transform-modules-commonjs": "^7.5.0",
    "@babel/plugin-transform-parameters": "^7.4.4",
    "@babel/plugin-transform-template-literals": "^7.4.4",
    "@babel/preset-env": "^7.22.10",
    "@babel/preset-typescript": "^7.22.5",
    "@date-fns/docs": "^0.29.0",
    "@octokit/core": "^5.0.2",
    "@size-limit/esbuild": "^11.0.1",
    "@size-limit/file": "^11.0.1",
    "@types/node": "^20.10.2",
    "@types/sinon": "^17.0.2",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@vitest/browser": "^0.34.6",
    "@vitest/coverage-v8": "^0.34.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^9.1.3",
    "babel-plugin-add-import-extension": "^1.6.0",
    "babel-plugin-add-module-exports": "^1.0.4",
    "babel-preset-power-assert": "^3.0.0",
    "cloc": "^2.2.0",
    "coveralls": "^3.1.1",
    "cross-env": "^7.0.3",
    "date-fns": "^3.0.4",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "firebase": "^10.7.1",
    "flow-bin": "^0.225.1",
    "fs-promise": "^2.0.3",
    "husky": "^8.0.3",
    "js-fns": "^2.5.1",
    "jsdoc-to-markdown": "^8.0.0",
    "json-loader": "^0.5.7",
    "karma": "^6.4.2",
    "karma-benchmark": "^1.0.4",
    "karma-benchmark-reporter": "^0.1.1",
    "karma-chrome-launcher": "~3.2.0",
    "karma-cli": "^2.0.0",
    "karma-mocha": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.4.0",
    "karma-webpack": "^5.0.0",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^10.2.0",
    "playwright": "^1.40.1",
    "power-assert": "^1.6.1",
    "prettier": "^3.1.0",
    "simple-git": "^3.21.0",
    "sinon": "^17.0.1",
    "size-limit": "^11.0.1",
    "tsx": "^4.6.1",
    "typedoc": "^0.25.4",
    "typedoc-plugin-missing-exports": "^2.1.0",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./index.d.mts",
        "default": "./index.mjs"
      }
    },
    "./constants": {
      "require": {
        "types": "./constants.d.ts",
        "default": "./constants.js"
      },
      "import": {
        "types": "./constants.d.mts",
        "default": "./constants.mjs"
      }
    },
    "./locale": {
      "require": {
        "types": "./locale.d.ts",
        "default": "./locale.js"
      },
      "import": {
        "types": "./locale.d.mts",
        "default": "./locale.mjs"
      }
    },
    "./fp": {
      "require": {
        "types": "./fp.d.ts",
        "default": "./fp.js"
      },
      "import": {
        "types": "./fp.d.mts",
        "default": "./fp.mjs"
      }
    },
    "./format": {
      "require": {
        "types": "./format.d.ts",
        "default": "./format.js"
      },
      "import": {
        "types": "./format.d.mts",
        "default": "./format.mjs"
      }
    },
    "./formatInTimeZone": {
      "require": {
        "types": "./formatInTimeZone.d.ts",
        "default": "./formatInTimeZone.js"
      },
      "import": {
        "types": "./formatInTimeZone.d.mts",
        "default": "./formatInTimeZone.mjs"
      }
    },
    "./getTimezoneOffset": {
      "require": {
        "types": "./getTimezoneOffset.d.ts",
        "default": "./getTimezoneOffset.js"
      },
      "import": {
        "types": "./getTimezoneOffset.d.mts",
        "default": "./getTimezoneOffset.mjs"
      }
    },
    "./toDate": {
      "require": {
        "types": "./toDate.d.ts",
        "default": "./toDate.js"
      },
      "import": {
        "types": "./toDate.d.mts",
        "default": "./toDate.mjs"
      }
    },
    "./utcToZonedTime": {
      "require": {
        "types": "./utcToZonedTime.d.ts",
        "default": "./utcToZonedTime.js"
      },
      "import": {
        "types": "./utcToZonedTime.d.mts",
        "default": "./utcToZonedTime.mjs"
      }
    },
    "./zonedTimeToUtc": {
      "require": {
        "types": "./zonedTimeToUtc.d.ts",
        "default": "./zonedTimeToUtc.js"
      },
      "import": {
        "types": "./zonedTimeToUtc.d.mts",
        "default": "./zonedTimeToUtc.mjs"
      }
    }
  }
}