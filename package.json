    "date-fns": "^3.0.6",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "firebase": "^10.7.1",
    "husky": "^8.0.3",
    "js-fns": "^2.5.1",
    "lint-staged": "^15.2.0",
    "lodash": "^4.17.15",
    "lodash.clonedeep": "^4.5.0",
    "mocha": "^10.2.0",
    "playwright": "^1.40.1",
    "prettier": "^3.1.0",
    "simple-git": "^3.21.0",
